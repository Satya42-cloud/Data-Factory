{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfscm"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls'"
		},
		"git_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'git_ls'"
		},
		"git_ls1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'git_ls1'"
		},
		"sql_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_ls'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0214-050359-vvkoc0kj"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://scmdataset.dfs.core.windows.net/"
		},
		"git_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Satya42-cloud/Data-Factory/refs/heads/main/Stream_Data/"
		},
		"git_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "satyajeetswain168@gmail.com"
		},
		"git_ls1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Satya42-cloud/Data-Factory/refs/heads/main/Static_Data/"
		},
		"git_ls1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "satyajeetswain168@gmail.com"
		},
		"sql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "scmserver12.database.windows.net"
		},
		"sql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "scmdatabase"
		},
		"sql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Satya"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfscm_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"adls_ls_accountKey": {
						"value": "[parameters('adls_ls_accountKey')]"
					},
					"git_ls_password": {
						"value": "[parameters('git_ls_password')]"
					},
					"git_ls1_password": {
						"value": "[parameters('git_ls1_password')]"
					},
					"sql_ls_password": {
						"value": "[parameters('sql_ls_password')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"git_ls_properties_typeProperties_url": {
						"value": "[parameters('git_ls_properties_typeProperties_url')]"
					},
					"git_ls_properties_typeProperties_userName": {
						"value": "[parameters('git_ls_properties_typeProperties_userName')]"
					},
					"git_ls1_properties_typeProperties_url": {
						"value": "[parameters('git_ls1_properties_typeProperties_url')]"
					},
					"git_ls1_properties_typeProperties_userName": {
						"value": "[parameters('git_ls1_properties_typeProperties_userName')]"
					},
					"sql_ls_properties_typeProperties_server": {
						"value": "[parameters('sql_ls_properties_typeProperties_server')]"
					},
					"sql_ls_properties_typeProperties_database": {
						"value": "[parameters('sql_ls_properties_typeProperties_database')]"
					},
					"sql_ls_properties_typeProperties_userName": {
						"value": "[parameters('sql_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}